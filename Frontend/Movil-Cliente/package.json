    "expo-status-bar": "~3.0.8",
    "expo-web-browser": "~15.0.8",
    "firebase": "^12.2.1",
    "install": "^0.13.0",
    "lottie-react-native": "^7.3.4",
    "lucide-react": "^0.540.0",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "^2.28.0",
