{
  "name": "movil-cliente",
  "version": "1.0.0",
  "main": "index.js",
  "expo": {
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Esta app necesita acceso a la galería para seleccionar fotos de perfil.",
          "cameraPermission": "Esta app necesita acceso a la cámara para tomar fotos de perfil."
        }
      ]
    ]
  },
  "scripts": {
    "start": "METRO_DISABLE_FS_WATCH=true expo start",
    "android": "METRO_DISABLE_FS_WATCH=true expo run:android",
    "ios": "METRO_DISABLE_FS_WATCH=true expo run:ios",
    "web": "METRO_DISABLE_FS_WATCH=true expo start --web"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
